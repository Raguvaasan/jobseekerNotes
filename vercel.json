{
    "version": 2,
    "builds": [{
        "src": "package.json",
        "use": "@vercel/next"
    }],
    "routes": [{
        "src": "/api/(.*)",
        "dest": "/api/$1"
    }],

    "env": {
        "DB_HOST": "DB_HOST",
        "DB_USER": "DB_USER",
        "DB_PASSWORD": "DB_PASSWORD",
        "DB_NAME": "DB_NAME"
    }


}